                      01732 ;
                      01733 ; Sending 8-bit data bytes in our 7-bit protocol
                      01734 ; (in data bytes, in other words) requires this
                      01735 ; escaping mechanism:
                      01736 ;   If high bit set, send $7E + low 7 bits
                      01737 ;   Literal $7E is sent as $7F + $7E
                      01738 ;   Literal $7F is sent as $7F + $7F
                      01739 ; 
                      01740 ; Other interesting cases inferred from the above:
                      01741 ;   $FE is sent as $7E + $7E
                      01742 ;   $FF is sent as $7E + $7F
                      01743 ;
                      01744 ; This calls the SIO_WRITE_W entry point, so the bank
                      01745 ; will shift as a result.
                      01746 ;
                      01747 SND8BIT MACRO   VALUE
                      01748         MOVLW   VALUE
                      01749         SEND_8_BIT_W
                      01750         ENDM
                      01751 
  00000001            01752 S8B_LABEL SET   1
                      01753 SEND_8_BIT_W MACRO
                      01754         MOVWF   EIGHTBITSIOBUF, ACCESS
                      01755         MOVLW   0x7F                            ; sending literal $7F?
                      01756         CPFSEQ  EIGHTBITSIOBUF, ACCESS
                      01757         BRA     S8B_CHK_7E#v(S8B_LABEL)
                      01758         CALL    SIO_WRITE_W                     ; send $7F to escape first
                      01759         MOVLW   0x7F                            ; then the literal $7F itself.
                      01760         CALL    SIO_WRITE_W                     
                      01761         BRA     S8B_END#v(S8B_LABEL)
                      01762 S8B_CHK_7E#v(S8B_LABEL):
                      01763         MOVLW   0x7E                            ; sending literal $7E?
                      01764         CPFSEQ  EIGHTBITSIOBUF, ACCESS
                      01765         BRA     S8B_CHK_MSB#v(S8B_LABEL)
                      01766         MOVLW   0x7F                            ; send $7F to escape first
                      01767         CALL    SIO_WRITE_W
                      01768         MOVLW   0x7E                            ; then the literal $7E itself.
                      01769         CALL    SIO_WRITE_W
                      01770         BRA     S8B_END#v(S8B_LABEL)
                      01771 S8B_CHK_MSB#v(S8B_LABEL):
