                      00109 ;       1. We run a 120 Hz timer which provides timing for the dimmers.
                      00110 ;
                      00111 ; MAIN_LOOP
                      00112 ;       Manages the display of the readerboards.
                      00113 ;       Updates the status of the SSR lines if it's time to do so
                      00114 ;       Receives a character from the serial line if one's waiting
                      00115 ;
                      00116 ; INTERPRETER
                      00117 ;       When a byte is received on the serial line, it is processed by
                      00118 ;       a small state machine.  The current state is held in YY_STATE (named
                      00119 ;       in honor of the venerable yacc).
                      00120 ;
                      00121 ;-----------------------------------------------------------------------------
                      00122 ; Command Protocol:
                      00123 ;                     ___7______6______5______4______3______2______1______0__
                      00124 ; Command Byte:      |      |                    |                           |
                      00125 ;                    |   1  |    Command code    |   Target device address   |
                      00126 ;                    |______|______|______|______|______|______|______|______|
                      00127 ;
                      00128 ; Any byte with its MSB set is the beginning of a command.  If the target 
                      00129 ; device matches this unit's address, the state machine kicks into gear and
                      00130 ; processes the command (which may require some following data bytes, all
                      00131 ; of which must have their MSB cleared).  Otherwise, the unit ignores the
                      00132 ; byte.
                      00133 ;
                      00134 ;                     ___7______6______5______4______3______2______1______0__
                      00135 ; Extended Command:  |      |                    |                           |
                      00136 ;                    |   1  |          7         |   Target device address   |
                      00137 ;                    |______|______|______|______|______|______|______|______|
                      00138 ;                    |      |                                                |
                      00139 ;                    |   0  |                  Command code                  |
                      00140 ;                    |______|______|______|______|______|______|______|______|
                      00141 ;
                      00142 ; The most common commands are given ID 0-6 so that they may be sent in as few
                      00143 ; bytes as possible (as few as a single byte), but we have more than 8 commands
                      00144 ; so we have an extended code.  If the command code is 7 (all bits set), then
                      00145 ; the following byte contains the actual command code which may be any value
                      00146 ; from 0-127.
                      00147 ;
                      00148 ;                     ___7______6______5______4______3______2______1______0__
                      00149 ; Data Byte:         |      |                                                |
                      00150 ;                    |   0  |                      Data                      |
                      00151 ;                    |______|______|______|______|______|______|______|______|
                      00152 ;
                      00153 ; Any byte with its MSB cleared is a data byte, and is ignored unless we're
                      00154 ; in the middle of interpreting a multi-byte command, in which case it's interpreted
                      00155 ; appropriately as data supporting the command being executed.  This way, other
                      00156 ; devices which share the same protocol format but not necessarily a compatible
                      00157 ; command set may safely know which bytes can be ignored without knowing the
                      00158 ; details of each other's command sets.
                      00159 ;
                      00160 ; Two special bytes are recognized:
                      00161 ;
                      00162 ;                     ___7______6______5______4______3______2______1______0__
                      00163 ; MSB Escape:        |      |                                                |
                      00164 ;                    |   0  |   1      1      1      1      1      1      0  |
                      00165 ;                    |______|______|______|______|______|______|______|______|
                      00166 ;
                      00167 ; If this ($7E) byte is received, it is ignored but the next byte received will
                      00168 ; have its MSB bit set.  This allows data bytes to have full 8-bit values without
                      00169 ; violating the communication protocol described above.  That second byte is not
                      00170 ; interpreted further.
                      00171 ;
                      00172 ;                     ___7______6______5______4______3______2______1______0__
                      00173 ; Literal Escape     |      |                                                |
                      00174 ;                    |   0  |   1      1      1      1      1      1      1  |
                      00175 ;                    |______|______|______|______|______|______|______|______|
                      00176 ;
                      00177 ; If this ($7F) byte is received, it is ignored but the next byte is accepted
                      00178 ; as-is without further interpretation.
                      00179 ;
                      00180 ; Specific Example Cases of interest:
                      00181 ;       Sequence    Resulting byte
                      00182 ;       $7E $7E     $FE
                      00183 ;       $7E $7F     $FF
                      00184 ;       $7F $7E     $7E
                      00185 ;       $7F $7F     $7F
                      00186 ;
                      00187 ; A command byte (received with MSB already set) trumps all of the above.  It is
                      00188 ; taken as the start of a command and the escape sequence in progress is canceled.
                      00189 ;
